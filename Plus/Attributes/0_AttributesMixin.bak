local Stats={}
local function Init_Value()
	local level = math.max(UnitLevel("Player"), 1)

    Stats={
        CR_CRIT_SPELL               = crt_cnv[level],--Crit - Melee 爆击 近战
        CR_HASTE_SPELL 	            = hst_cnv[level],--Haste - Melee 急速
        CR_VERSATILITY_DAMAGE_DONE  = vrs_cnv[level],--Versatility - Damage Done 全能，伤害
        CR_MASTERY                  = mst_cnv[level],--Mastery 精通
        CR_LIFESTEAL	            = lee_cnv[level],--Lifesteal 吸血
        CR_AVOIDANCE                = avd_cnv[level],--Avoidance 闪避
        CR_SPEED                    = spd_cnv[level],--Speed 速度
    }
end

C_Timer.After(2, function()
    if TimerunningUtil.TimerunningEnabledForPlayer() then--幻境新生
        return
    end

    Init_Value()


    if select(4, GetBuildInfo())~=DataVersion and WoWTools_DataMixin.Player.husandro then
        print(WoWTools_DataMixin.Icon.icon2
            ..'|cnWARNING_FONT_COLOR:0_AttributesMixin.lua '..(WoWTools_DataMixin.onlyChinese and '数据需要更新' or 'The data needs to be updated'),
            '|cffffffffver',
            DataVersion
        )
    end
    EventRegistry:RegisterFrameEventAndCallback("PLAYER_LEVEL_UP", Init_Value)
end)

--[[
    addon.BaseCritRating = GetCombatRating(CR_CRIT_SPELL);
    addon.BaseHasteRating = GetCombatRating(CR_HASTE_SPELL);

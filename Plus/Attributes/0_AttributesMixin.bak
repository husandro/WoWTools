	addon.BaseLeechRating = GetCombatRating(CR_LIFESTEAL);
	addon.BaseAvoidanceRating = GetCombatRating(CR_AVOIDANCE);
	addon.BaseSpeedRating = GetCombatRating(CR_SPEED);
]]

